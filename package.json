{
  "name": "graphql-mongodb-server",
  "version": "1.0.0",
  "description": "A GraphQL MongoDB server.",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/server/index.ts",
    "server": "nodemon --exec babel-node src/server/index.ts",
    "graphql:gen": "graphql-codegen --config codegen.yml"
  },
  "keywords": [
    "graphql",
    "mongodb",
    "boilerplate",
    "server"
  ],
  "license": "MIT",
  "devDependencies": {
    "@oakslab/ooo-config": "0.0.6",
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-dom": "^2.1.1",
    "react-testing-library": "^5.2.3",
    "@graphql-codegen/typescript": "1.3.1",
    "@graphql-codegen/typescript-resolvers": "1.3.1",
    "@graphql-codegen/typescript-mongodb": "1.3.1",
    "@graphql-codegen/introspection": "1.3.1"
  },
  "dependencies": {
    "@types/mongoose": "^5.5.7",
    "concurrently": "^4.1.0",
    "dotenv": "^6.1.0",
    "graphql": "^14.4.2",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tools": "^4.0.3",
    "graphql-yoga": "^1.16.7",
    "merge-graphql-schemas": "^1.5.7",
    "mongoose": "^5.3.13",
    "nodemon": "^1.18.10",
    "typescript": "^3.3.3",
    "uuid": "^3.3.2"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/config/tests/jest.config"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
